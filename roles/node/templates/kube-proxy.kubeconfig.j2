apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: {{ kube_cert_dir }}/ca.crt
    server: {{ kube_apiserver }}
  name: inu-k8s
contexts:
- context:
    cluster: inu-k8s
    user: kube-proxy
  name: kube-proxy-context
users:
- name: kube-proxy
  user:
    token: {{ kube_proxy_token }}
current-context: kube-proxy-context